<configuration>
	<system.webServer>

		<!-- indicates that the server.js file is a node.js application 
		to be handled by the iisnode module -->

		<handlers>
			<add name="iisnode" path="Main.js" verb="*" modules="iisnode" />
		</handlers>
		
		<rewrite>
			<rules>
				<rule name="myapp">
					<match url="/*" />
					<action type="Rewrite" url="Main.js" />
				</rule>
			</rules>
		</rewrite>
 
	</system.webServer>
</configuration>
